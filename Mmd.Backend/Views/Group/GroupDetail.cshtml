        <div class="Lorelei-table-box-c mgc">

            <div class="LoreleitopMenu">
                <a class="cur" href=@Url.Action("GroupManage", "Group", new {status = (int) EGroupStatus.已发布})>返回</a>
            </div>
            <div class="lineWidget"></div>
            <div id="list_table">
                @Html.Action("GroupGetDetailPartial", "Group", new { pageIndex = 1, gid= Model })
