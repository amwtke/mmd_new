    <title>详情</title>
    <link rel="stylesheet" type="text/css" href="/Content/main.css">
    @Scripts.Render("~/bundles/jquery")
    <script src="/Scripts/group/gdpaging.js"></script>
</head>
<body>
    <!--header-->
    @Html.Partial("Header/_headerGroup")

    <!--mian-->
