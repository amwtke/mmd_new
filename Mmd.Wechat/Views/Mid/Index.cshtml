
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>商家</title>
    @Scripts.Render("~/bundles/jquery")
    <script src="/Scripts/mid/common.js"></script>
</head>
<body>
<p>
    <span><h1>商家:@ViewBag.mName</h1></span>
    <br>
    访问量：<input id="mid" type="text" value=@ViewBag.mid style="display: none"/><input id="fwDays" type="text"/> 天内。<button id="btfw" onclick="midFW()">查询</button>
